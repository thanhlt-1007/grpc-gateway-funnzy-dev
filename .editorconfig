end_of_line = "cr"
charset = "utf-8"
trim_trailing_whitespace = true
insert_final_newline = true
root = true

[*.go]
indent_style = "tab"
indent_size = 8
tab_width = 8

[go.mod]
indent_style = "tab"
indent_size = 8
tab_width = 8

[go.sum]
indent_style = "tab"
indent_size = 8
tab_width = 8

[*.proto]
indent_style = "space"
indent_size = 2
tab_width = 2

[*.json]
indent_style = "space"
indent_size = 2
tab_width = 2

[Makefile]
indent_style = "tab"
indent_size = 2
tab_width = 2
